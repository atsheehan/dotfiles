[alias]
  s = status
  co = checkout
  cob = checkout -b
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  check = diff --staged
  add-check = !git add -A && git check

[color]
  branch = auto
  diff = auto
  iteractive = auto
  status = auto

[github]
  user = atsheehan

[core]
  excludesfile = ~/.gitignore
  pager = less -r

[push]
  default = matching
